{
    "$schema": "https://mintlify.com/docs.json",
    "theme": "palm",
    "name": "Ducky",
    "colors": {
        "primary": "#F5FF82",
        "light": "#F5FF82",
        "dark": "#F5FF82"
    },
    "favicon": "/favicon.svg",
    "navigation": {
        "tabs": [
            {
                "tab": "Guides",
                "groups": [
                    {
                        "group": "Get Started",
                        "pages": [
                            "index",
                            "quickstart"
                        ]
                    },
                    {
                        "group": "Essentials",
                        "pages": [
                            "essentials/markdown",
                            "essentials/code",
                            "essentials/images",
                            "essentials/settings",
                            "essentials/navigation",
                            "essentials/reusable-snippets"
                        ]
                    }
                ]
            }
        ],
        "global": {
            "anchors": [
                {
                    "anchor": "Documentation",
                    "href": "https://mintlify.com/docs",
                    "icon": "book-open-cover"
